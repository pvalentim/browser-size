{
  "name": "browser-size",
  "version": "0.0.1",
  "description": "Get the window size of the browser and throttled resize events.",
  "main": "index.js",
  "scripts": {
    "compile": "babel index.js > dist/index.js",
    "test": "browserify test/index.js | testling -u"
  },
  "testling": {
    "browsers": [
      "ie/10"
      "firefox/latest",
      "chrome/latest",
      "safari/6..latest",
      "iphone/latest",
      "ipad/latest",
      "android-browser/latest"
    ],
    "files": "test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pvalentim/browser-size.git"
  },
  "keywords": [
    "browser",
    "window",
    "resize",
    "responsive"
  ],
  "author": "Pedro Valentim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pvalentim/browser-size/issues"
  },
  "homepage": "https://github.com/pvalentim/browser-size",
  "dependencies": {
    "lodash.throttle": "^3.0.1",
    "pragma-singleton": "^1.0.3"
  },
  "devDependencies": {
    "babel": "^4.0.2",
    "tape": "^3.5.0"
  }
}
